#include "main.h"

/**
 **_memset - function that fills memory with a constant byte
 *@s: character variable
 *@b: second character argument
 *@n: integer variable for the byte
 *Return: value
 */
char *_memset(char *s, char b, unsigned int n)
{
int x = 0;
 for (; n > 0; x++)
{
s[x] = b;
n--;
}
return (s);
}
